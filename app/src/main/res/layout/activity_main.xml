<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainnobt"
    android:orientation="vertical"
    tools:context="wasddev.appwasd.MainActivity">

    <ImageButton
        android:id="@+id/btEntrarM"
        android:layout_width="400dp"
        android:layout_height="110dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="440dp"
        app:srcCompat="@drawable/btentrar"
        android:onClick="activity_login"/>

    <ImageButton
        android:id="@+id/btInscreverM"
        android:layout_width="400dp"
        android:layout_height="110dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="95dp"
        app:srcCompat="@drawable/btinscreva" />
</LinearLayout>
